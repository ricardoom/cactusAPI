{
  "plugins": ["import"],
  "rules": {
    "comma-dangle": ["error", { "objects": "only-multiline" }],
    "import/no-unresolved": 2,
    "import/no-commonjs": 2,
    "import/extensions": [2, "ignorePackages"]
  },
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  }
}
